<h2>登录</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :用户名 %><br />
    <%= f.text_field :username, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :密码 %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>


  <div class="actions">
    <%= f.submit "登录" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
